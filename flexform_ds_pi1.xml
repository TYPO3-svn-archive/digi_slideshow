<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
<sheets>
  <sTAB1>
    <ROOT>
      <TCEforms>
        <sheetTitle>LLL:EXT:digi_slideshow/locallang_db.xml:tt_content.pi_flexform.tab1</sheetTitle>
      </TCEforms>
      <type>array</type>
      <el>
        <number>
          <TCEforms>
            <label>LLL:EXT:digi_slideshow/locallang_db.xml:tt_content.pi_flexform.number</label>
            <config>
              <type>input</type>
              <eval>int</eval> 
              <default>1</default>
              <maxitems>1</maxitems>
              <size>1</size>
            </config>
          </TCEforms>
        </number>
        <interval>
          <TCEforms>
            <label>LLL:EXT:digi_slideshow/locallang_db.xml:tt_content.pi_flexform.interval</label>
            <config>
              <type>input</type>
              <eval>int</eval> 
              <range>
                <upper>10000</upper>
                <lower>1</lower>
              </range>
              <default>5000</default>
              <maxitems>1</maxitems>
              <size>1</size>
            </config>
          </TCEforms>
        </interval>
        <duration>
          <TCEforms>
            <label>LLL:EXT:digi_slideshow/locallang_db.xml:tt_content.pi_flexform.duration</label>
            <config>
              <type>input</type>
              <eval>int</eval>
              <range>
                <upper>10000</upper>
                <lower>1</lower>
              </range>
              <default>500</default>
              <maxitems>1</maxitems>
              <size>1</size>
            </config>
          </TCEforms>
        </duration>
      </el>
    </ROOT>
  </sTAB1>
  <sTAB2>
    <ROOT>
      <TCEforms>
        <sheetTitle>LLL:EXT:digi_slideshow/locallang_db.xml:tt_content.pi_flexform.tab2</sheetTitle>
      </TCEforms>
      <type>array</type>
      <el>
        <imagelist>
          <TCEforms>
            <label>LLL:EXT:digi_slideshow/locallang_db.xml:tt_content.pi_flexform.images</label>
            <config>
              <type>group</type>
              <internal_type>file</internal_type>
              <allowed>gif,jpg,jpeg,tif,bmp,pcx,tga,png,pdf,ai</allowed>
              <max_size>1024</max_size>
              <uploadfolder>uploads/tx_digislideshow</uploadfolder>
              <show_thumbs>1</show_thumbs>
              <maxitems>99</maxitems>
              <size>5</size>
            </config>
          </TCEforms>
        </imagelist>
      </el>
    </ROOT>
  </sTAB2>
</sheets>
</T3DataStructure>